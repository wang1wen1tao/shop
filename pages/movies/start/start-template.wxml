<template name="startTemplate">
  <view class="star-container">
    <view class="stars">
    <block wx:for="{{start}}" wx:for-item="i">
    <image wx:if="{{i}}"src="/images/icon/star.png"></image>
    <image wx:else src="/images/icon/none-star.png"></image>
    </block>
    </view>
    <text class="stars-score">{{avarea}}</text>
  </view>
</template>